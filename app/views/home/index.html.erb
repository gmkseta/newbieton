


<table>
	<thead>
	<tr><!---->
		<th>이름</th>
		<th>학교</th>
		<th>학과</th>
	</tr>
	
	
	<%@professors.each do |p|%>
		<tr>	
			<td><a href = "/home/new_score/<%=p.id%>"><%=p.name%></a></td>
			<td><%=p.college%></td>
			<td><%=p.major%></td>
			<%p.scores.each do |s|%>
				<td><%=s.attend%></td>
			<%end%>
			
		</tr>
	<%end%>
		
	
</table>

